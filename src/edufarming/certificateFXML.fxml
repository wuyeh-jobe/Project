<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="edufarming.CertificateFXMLController">
   <children>
      <ImageView fx:id="cert_img" fitHeight="536.0" fitWidth="722.0" layoutY="72.0">
         <image>
            <Image url="@of%20achievement.png" />
         </image>
      </ImageView>
      <Label fx:id="name_lbl" layoutX="45.0" layoutY="341.0" prefHeight="55.0" prefWidth="429.0" textFill="#7d9778">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label fx:id="percScore_lbl" layoutX="44.0" layoutY="441.0" prefHeight="27.0" prefWidth="69.0" textFill="#7d9778">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <Button fx:id="logout_btn" layoutX="642.0" layoutY="14.0" mnemonicParsing="false" onAction="#logOut" text="Logout" />
      <Button fx:id="home_btn" layoutX="23.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToHomepage" text="Home" textFill="#789784" />
   </children>
</AnchorPane>
